    "import fitz\n",
    "\n",
    "def extract_text_from_pdf(pdf_path):\n",
    "    doc = fitz.open(pdf_path)\n",
    "    text= \"\\n\".join(page.get_text() for page in doc).strip()\n",
    "    return preprocess_cv_text(text)\n"
   ]
